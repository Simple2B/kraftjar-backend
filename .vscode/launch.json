{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "API",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "api:app",
        "--reload",
        "--port",
        "5002"
      ],
      "justMyCode": true,
      "env": {
        "IS_API": "true"
      }
    },
    {
      "name": "PyTest",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "justMyCode": true
    },
    {
      "name": "Flask",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "wsgi:app",
        "APP_ENV": "development",
        "FLASK_DEBUG": "1"
      },
      "args": [
        "run",
        "--port",
        "5000"
      ],
      "jinja": true,
      "justMyCode": false
    },
    {
      "name": "Export services",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "args": [
        "export-services"
      ],
    },
    {
      "name": "Fill DB",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "args": [
        "fill-stage-db"
      ],
    },
    {
      "name": "Write phone numbers",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "args": [
        "write-phones"
      ],
    }
  ]
}
